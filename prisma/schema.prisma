generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Contact {
  id    String     @id @default(uuid())
  email String  @unique
  name  String
  message String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
